{
	"name": "kitsuniru-theme",
	"displayName": "Kitsuniru Theme",
	"description": "A beautiful dark theme for Visual Studio Code",
	"version": "0.0.4",
	"publisher": "Kitsuniru",
	"author": {
		"name": "Artemis Kushnere",
		"email": "kitsushimo.dev@gmail.com"
	},
	"activationEvents": [],
	"main": "./out/extension.js",
	"icon": "logo.jpg",
	"galleryBanner": {
		"color": "#15141b",
		"theme": "dark"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript-eslint": "^8.16.0"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"theme",
		"dark",
		"kitsuniru-theme",
		"color-theme"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"themes": [
			{
				"label": "Kitsuniru Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/kitsuniru-dark-color-theme.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kitsuniru/kitsuniru-theme.git"
	},
	"bugs": {
		"url": "https://github.com/kitsuniru/kitsuniru-theme/issues"
	}
}
